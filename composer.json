{
    "name": "fabriziocaldarelli/yii2-vueapp-moment",
    "description": "Moment plugin for Yii2 VueApp package",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","vue", "moment"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Fabrizio Caldarelli",
            "email": "fabrizio.caldarelli@gmail.com"
        }
    ],
    "require": {
        "npm-asset/moment": "^2.24"
    },
    "require-dev": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "<7"
    },    
    "autoload": {
        "psr-4": {
            "sfmobile\\vueapp\\assets\\moment\\": "src"
        }
    },   
    "autoload-dev": {
        "psr-4": {
            "sfmobile_unit\\vueapp\\assets\\moment\\": "tests"
        }
    },    
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],    
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }    
}
